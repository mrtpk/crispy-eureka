{
  "store_features_basedir": "features",
  "model": "lodnn",
  "dataset": "kitti",
  "view": "bev",
  "parameters":{
    "compute_classic": "True",
    "add_geometrical_features": "True",
    "subsample_ratio": 1,
    "compute_eigen": 100
  },
  "experiments": ["classic", "classic+geometry", "classic+eigen","classic+geometry+eigen"],
  "gt_args": {
    "classes": [40]
  },
  "training_config":{
    "loss_function": "binary_crossentropy",
    "learning_rate": 1e-4,
    "batch_size": 8,
    "epochs": 100,
    "optimizer": "keras.optimizers.Nadam"
  }
}
